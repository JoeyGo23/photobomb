<div class="container">

<h1><i class="fa fa-camera-retro"></i>  PhotoBomb</h1>
<h3>Share photos. Share inspiration.</h3>

<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>

  <div>
  <%= f.text_field :username %><br />
  <%= f.label :username %>
</div>

  <div>
  <%= f.password_field :password %><br />
  <%= f.label :password %>
</div>


  <div class="submit"><%= f.submit "Sign in" %></div>
<% end %>

<%= render "devise/shared/links" %>

</div>
